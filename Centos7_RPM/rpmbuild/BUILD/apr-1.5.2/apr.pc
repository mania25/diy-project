prefix=/usr
exec_prefix=/usr
libdir=/usr/lib64
APR_MAJOR_VERSION=1
includedir=/usr/include/apr-1

Name: APR
Description: The Apache Portable Runtime library
Version: 1.5.2
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -lrt -lcrypt  -lpthread -ldl
Cflags: -DLINUX -D_REENTRANT -D_GNU_SOURCE  -pthread -I${includedir}
